{
  "name": "toychain",
  "version": "0.0.1",
  "description": "A PoW blockchain.",
  "bin": {
    "toychain": "bin/toychain.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest --watchAll",
    "start": "node bin/toychain",
    "dev": "yarn start:redis && nodemon bin/toychain",
